<div class="jumbotron text-center">
	<div class="container">
		<h1 class="display-4"></h1>
		<H1>Strawberry Money Tracker etc</H1>
	</div>
	<br>
</div>
{{#if user}}

{{else}}
	{{#if feedbacks}}
	{{#each feedbacks}}
	<div class="card border-primary mb-2 w-100 mx-auto">

		<div class="card-body">

			<div class="row">

				<div class="col-sm-12 col-md-12 col-lg-12">
					<div class="row">
						<div class="col-sm-12 col-md-6 col-lg-1">
							<p class="font-italic">Title</p>
							<p class="text-info" id="titles">
								{{Title}}
							</p>
						</div>
						<div class="col-sm-12 col-md-6 col-lg-4">
							<p class="font-italic">Body</p>
							<p class="text-info"> {{Textfeed}} </p>

						</div>
						<div class="col-sm-12 col-md-6 col-lg-4">
							<p class="font-italic">Suggestion</p>
							<p class="text-info">{{Suggestion}}</p>
						</div>
						{{!-- ratings  --}}
						<div class="col-sm-12 col-md-6 col-lg-1">
							<p class="font-italic">Ratings</p>
							<p class="text-info" id="myChart">{{Rating}} / 5</p>
						</div>


						<div class="col-sm-12 col-md-6 col-lg-2">
							<p class="font-italic">Date</p>
							<p class="text-info">{{formatDate Date 'DD/MM/YYYY' }}</p>
							{{!-- formatDate Date 'DD/MM/YYYY' --}}
						</div>

					</div>
				</div>
				{{!-- <div class="col-sm-12 col-md-6 col-lg-2">
							<p class="font-italic"></p>
							<a href="/video/edit/{{id}}" class="card-link">
								<i class="fas fa-pencil-alt"></i>
								Edit
							</a>
							<a href="/video/delete/{{id}}" class="card-link" data-toggle="confirmation"
								data-title="Confirm delete?">
								<i class="far fa-trash-alt"></i>
								Delete
							</a>
						</div> --}}
			</div>
		</div>

	</div>
	{{/each}}
	{{else}}
	</div>
	<h3>No Feedback Recorded yet</h3>
	</div>
	{{/if}}
{{/if}}