<script>
	$('#datepicker').datepicker({
		format: 'dd/mm/yyyy',
		value: new Date().toLocaleDateString('en-SG'),
		uiLibrary: 'bootstrap4',
	});
</script>
<script>
	$('#datepicker2').datepicker({
		format: 'dd/mm/yyyy',
		value: new Date().toLocaleDateString('en-SG'),
		uiLibrary: 'bootstrap4',
	});
</script>

<script>
	$(document).ready(function () {

		$("#search").click(function () {
			console.log("hit");
			var query = $("#query").val();
			$.ajax({
				url: "/transactionH/search/" + query,
				type: "GET",
				success: function (data) {
					console.log(data);
					var dashboards = data.dashboard;
					$("#card").html("");
					$.each(dashboards, function (index, dashboard) {
						var row = `
							<div class="card-header">
								<h5 class="card-title">${dashboard.Name}</h5>
							</div>
							<div class="card-body">
								{{!-- Practical 09 Exercise 02 --}}
								<div class="row">
									
									<div class="col-sm-12 col-md-12 col-lg-8">
										<div class="row"> {{!-- subtitles, language... --}}
											<div class="col-sm-12 col-md-6 col-lg-3">
												<p class="font-italic">Subtitles</p>
												<p class="text-info">
													${dashboard.Notes}
												</p>
											</div>
											<div class="col-sm-12 col-md-6 col-lg-4">
												<p class="font-italic">Language</p>
												<p class="text-info">
													${dashboard.Tags}
												</p>
											</div>
											<div class="col-sm-12 col-md-6 col-lg-2">
												<p class="font-italic">Class</p>
												<p class="text-info">${dashboard.Amount}</p>
											</div>
											<div class="col-sm-12 col-md-6 col-lg-3">
												<p class="font-italic">Date Release</p>
												<p class="text-info">${dashboard.Date}</p>
					
											</div >
										</div >
						<div class="row">
							<div class="col-sm-12 col-md-6 col-lg-10">
								<p class="font-italic">Starring</p>
								<p class="text-info">${dashboard.Name}</p>
							</div>
						</div>
						<div class="row">
							<div class="col-sm-12 col-md-6 col-lg-10">
								<p class="font-italic">Type</p>
								<p class="text-info">{{PlusMinus}}</p>
							</div>
						</div>
					</div>
					<div class="col-sm-12 col-md-6 col-lg-2">
						<p class="font-italic"></p>
						<a href="/video/edit/{{id}}" class="card-link">
							<i class="fas fa-pencil-alt"></i>
							Edit
						</a>
						<a href="/transactionH/delete/{{id}}" class="card-link" data-toggle="confirmation"
							data-title="Confirm delete?">
							<i class="far fa-trash-alt"></i>
							Delete
						</a>
					</div>
				</div>
			</div>

							`
							console.log(row);
							$("#card").append(row);
						})
					}
				})
			})
		})
</script>




