<div class="container">
	{{#if dashboard}}
	<div class="card card-body">
		<h3 class="text-primary">asdjkl</h3>
		{{#each dashboard}}
		<div class="card border-primary mb-2 w-100 mx-auto">
			<div class="card-header">
				<h5 class="card-title">Age: {{CheckorSave}}</h5>
                <h5 class="card-title">Monthly income: {{Amount}}</h5>
                <h5 class="card-title">Plan to save monthly: {{Tags}}</h5>
                <h5 class="card-title">Living expense: {{Notes}}</h5>
                <h5 class="card-title">Food expense: {{Date}}</h5>
		    </div>
        	</div>
            {{/each}}
        </div>
	    {{else}}
	    <h3>No budgeting set up</h3>
	{{/if}}
</div>