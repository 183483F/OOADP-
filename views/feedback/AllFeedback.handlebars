<style>
	.rate {
		float: left;
		height: 46px;
		padding: 0 10px;
	}

	.rate:not(:checked)>input {
		position: absolute;
		top: -9999px;
	}

	.rate:not(:checked)>label {
		float: right;
		width: 1em;
		overflow: hidden;
		white-space: nowrap;
		cursor: pointer;
		font-size: 30px;
		color: #ccc;
	}

	.rate:not(:checked)>label:before {
		content: 'â˜… ';
	}

	.rate>input:checked~label {
		color: #ffc700;
	}

	.rate:not(:checked)>label:hover,
	.rate:not(:checked)>label:hover~label {
		color: #deb217;
	}

	.rate>input:checked+label:hover,
	.rate>input:checked+label:hover~label,
	.rate>input:checked~label:hover,
	.rate>input:checked~label:hover~label,
	.rate>label:hover~input:checked~label {
		color: #c59b08;
	}
</style>


<body >
	

	{{#if feedbacks}} {{!-- from models feedback ,db.define('feedback') --}}

	<div class="card card-body" style="background-color:white;">


		<div style="width:500px; height:300px;float:left;">

			<canvas id="myChart" {{!-- style="width:30px; height:30px;float:left;" --}}></canvas>

		</div>

<div class="row">
		<h3 class="text-primary" style="float:left;padding-left:20px;">All Feedbacks</h3>
		<h3 style="float:right;"> Total {{sumz}} Comments</h3>
		</div>
		{{!-- <div class="mb-2">
			<input type="text" id="titles" onkeyup="showHint(this.value)">
		</div> --}}

		{{#each feedbacks}}
		<div class="container">
			<div class="card border-primary mb-2 w-100 mx-auto">

				<div class="card-body" style="background-color: white;">
					{{!-- <div class="row"> --}}
					<div class="col-sm-12 col-md-12 col-lg-12">
						<div class="row">
							<div class="col-sm-12 col-md-12 col-lg-8">
								{{!-- ratings  --}}
								{{!-- <p class="font-italic">Ratings</p> --}}
								{{!-- <div class="row"> --}}
								{{#ifEquals Rating "5"}}
								<i class="fa fa-star" style="color: #ffc700;"  id="myChart" value="1" title="1 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="2" title="2 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="3" title="3 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="4" title="4 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="5" title="5 Stars"></i>
								{{/ifEquals}}
								{{#ifEquals Rating "4"}}
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="1" title="1 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="2" title="2 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="3" title="3 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="4" title="4 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="5" title="5 Stars"></i>
								{{/ifEquals}}
								{{#ifEquals Rating "3"}}
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="1" title="1 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="2" title="2 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="3" title="3 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="4" title="4 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="5" title="5 Stars"></i>
								{{/ifEquals}}
								{{#ifEquals Rating "2"}}
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="1" title="1 Stars"></i>
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="2" title="2 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="3" title="3 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="4" title="4 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="5" title="5 Stars"></i>
								{{/ifEquals}}
								{{#ifEquals Rating "1"}}
								<i class="fa fa-star" style="color: #ffc700;" id="myChart" value="1" title="1 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="2" title="2 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="3" title="3 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="4" title="4 Stars"></i>
								<i class="fa fa-star" style="color: grey;" id="myChart" value="5" title="5 Stars"></i>
								{{/ifEquals}}
								&nbsp; &nbsp; &nbsp;
								<p class="text-info" id="myChart">{{Rating}} out of 5 Stars</p>
								{{!-- </div> --}}
							</div>

							<div class="col-sm-12 col-md-12 col-lg-4">
								<h4 title="Username" class="text-info" id="titles" style="float:right;">
									{{UserName}}
								</h4>
							</div>
						</div>
					</div>
					<hr>
					{{!-- </div> --}}
					<div class="row">
						<div class="col-sm-12 col-md-6 col-lg-2">
							<p class="font-italic">Title:</p>
							<p class="text-info" id="titles">
								{{Title}}
							</p>
						</div>


						<div class="col-sm-12 col-md-6 col-lg-8">
							<p class="font-italic">Comments:</p>
							<p class="text-info"> {{Textfeed}} </p>

							<p class="font-italic">Suggestion:</p>
							<p class="text-info">{{Suggestion}}</p>

						</div>


						<div class="col-sm-12 col-md-6 col-lg-2">
							<p class="font-italic">Date:</p>
							<p class="text-info">{{formatDate Date 'DD/MM/YYYY' }}</p>
							{{!-- formatDate Date 'DD/MM/YYYY' --}}

						</div>
					</div>

					<div class="col-sm-12 col-md-6 col-lg-12">
						<hr>
						<p class="font-italic"></p>
						<a href="/Feedback/edit/{{id}}" class="card-link">
							<i class="fas fa-pencil-alt"></i>
							Edit
						</a>
						<a href="/Feedback/delete/{{id}}" class="card-link" data-toggle="confirmation"
							data-title="Confirm delete?">
							<i class="far fa-trash-alt"></i>
							Delete
						</a>
					</div>

				</div>
			</div>
		</div>

		{{/each}}
	</div>

	{{else}}
	<h3>No Feedback History</h3>

	{{/if}}

</body>